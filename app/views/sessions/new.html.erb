<div class="start-pages signup-login">
  <h4 class="blue-bold">Log In to Traveler</h4><br>

    <%= form_for :user do |f| %>
        <%= f.email_field :email, placeholder: "Email", :size => 30 %>
        <%= f.password_field :password, placeholder: "Password", :size => 30 %>
        <%= f.submit "Continue with Email", :class => 'btn btn-block btn-success' %><br>
    <% end %>
</div>


